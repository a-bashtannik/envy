{
    "name": "worksome/envsync",
    "description": "Automatically keep your .env.example file in sync.",
    "keywords": [
        "worksome",
        "laravel",
        "envsync"
    ],
    "homepage": "https://github.com/worksome/envsync",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Downing",
            "email": "luke@worksome.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-parallel": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.26",
        "worksome/coding-style": "0.6.2"
    },
    "autoload": {
        "psr-4": {
            "Worksome\\Envsync\\": "src",
            "Worksome\\Envsync\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Worksome\\Envsync\\Tests\\": "tests"
        }
    },
    "scripts": {
        "lint": "vendor/bin/phpcbf",
        "test:unit": "vendor/bin/pest -p",
        "test:coverage": "vendor/bin/pest -p --coverage",
        "test:types": "vendor/bin/phpstan analyse",
        "test:style": "vendor/bin/phpcs -n",
        "test": [
            "@test:style",
            "@test:types",
            "@test:unit"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Worksome\\Envsync\\EnvsyncServiceProvider"
            ],
            "aliases": {
                "Envsync": "Worksome\\Envsync\\Facades\\Envsync"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
